## Raise(exc, cause)
raise
raise gg
raise x from y

## Assert(test, msg)
assert x
assert x,y


## Delete(targets)
del x,y,z

## Pass
def thing():
    pass

## Import(names)
import x,y,z

## ImportFrom(module, names, level)
from y import x,y,z

## import from with params
from y import (x,y,z)

## alias(name, asname)
